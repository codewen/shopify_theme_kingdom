/*-------------------------
    1. GENERAL
*------------------------*/

/*!
 *  1.1. Reset
*/

html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{border:0;font-size:100%;font:inherit;vertical-align:baseline;margin:0;padding:0}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section,canvas,main{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:none}table{border-collapse:collapse;border-spacing:0}a{text-decoration:none}*:focus{outline:none}*::-moz-focus-inner {border:0;}

%clearfix {
  *zoom: 1;
  &:before, &:after {
    content: " ";
    display: table;
  }
  &:after {
    clear: both;
  }
}


.clearfix {
	@extend %clearfix;
}

.hidden {
   display: none;
}
#oldie {
   display: none;
}
.clear {
   clear: both;
}

* {
	box-sizing: border-box;
}

/*!
 *  1.2. Variables
*/

// Fonts

$headingsFont: {{ settings.typography_headings }};
$menuFont: {{ settings.typography_menus }};
$bodyFont: {{ settings.typography_body }};
$quotesFont: {{ settings.typography_body }};

// Sidebar

$sidebarBackground: {{ settings.sidebar_bg }};
$sidebarIconsColor: lighten({{ settings.sidebar_text }}, 15%);
$sidebarIconsColorHover: darken({{ settings.sidebar_text }}, 15%);
$logoColor: {{ settings.logo_color }};

$collectionsMenu: {{ settings.menus_1 }};
$collectionsSubmenu: lighten({{ settings.menus_1 }}, 20%);
$pagesMenu: {{ settings.menus_3 }};
$pagesSubmenu: {{ settings.menus_3 }};
$menuRollOver: {{ settings.menus_5 }};
$menuSelected: {{ settings.menus_6 }};
$menuBorder: darken({{ settings.sidebar_bg }}, 3%);

$sidebarText: {{ settings.sidebar_text }};
$sidebarLinks: {{ settings.sidebar_links }};

// General

$pageBackground: {{ settings.page_bg }};
$headingsColor: {{ settings.type_headings }};
$bodyColor: {{ settings.type_body }};
$bodyExtraColor: {{ settings.type_additional }};
$linksColor: {{ settings.type_links }};

$collectionsTitle: {{ settings.type_body }};
$collectionsOption: {{ settings.type_additional }};

// Forms & Buttons

$formsBack: {{ settings.form_back }};
$formsStyle: {{ settings.form_style }};
$formsBorder: {{ settings.form_border }};
$formsLabel: {{ settings.form_label }};
$mainButtons: {{ settings.button_color }};
$secondaryButtons: {{ settings.button_dark }};

@function set-button-text-color( $color ) {
	@if ( lightness( $color ) > 55 ) {
		@return #000;
	} @else {
		@return #fff;
	}
}

@function calculate-line-height( $size ) {
	@if ( $size < 44 ) {
		@return $size + 4 + px;
	} @else {
		@return $size + px;
	}
}

// Other

$dividersColor: darken({{ settings.page_bg }}, 3%);
$comparePrice: {{ settings.type_additional }};
$blockquoteColor: darken({{ settings.type_body }}, 10%);

/*!
 *  1.3. Mixins
*/

@mixin scale($scale, $origin) {
	-webkit-transform: scale($scale);
	-ms-transform: scale($scale);
	transform: scale($scale);
	-webkit-transform-origin: $origin;
	-ms-transform-origin: $origin;
	transform-origin: $origin;
}

@mixin rotate($rotation) {
	-webkit-transform: rotate($rotation);
	-ms-transform: rotate($rotation);
	transform: rotate($rotation);
}

@mixin translate3d($x, $y, $z) {
	-webkit-transform: translate3d($x, $y, $z);
	-ms-transform: translate3d($x, $y, $z);
	transform: translate3d($x, $y, $z);
}

@mixin opacity($opacity) {
	opacity: $opacity;
}

@mixin alphaback($color, $opacity) {
	background: $color;
	background: rgba($color, $opacity);
}

@mixin prefix($property, $value) {
	-moz-#{$property}: #{$value};
	-webkit-#{$property}: #{$value};
	#{$property}: #{$value};
}

@mixin bgimg(){
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center center;
}

@mixin transition($property, $speed, $easing) {
	-webkit-transition: $property $speed $easing;
	transition: $property $speed $easing;
}	

@mixin folioanim() {
	-webkit-transition: all 200ms cubic-bezier(0.420, 0.000, 1.000, 1.000);
	transition: all 200ms cubic-bezier(0.420, 0.000, 1.000, 1.000); 
	-webkit-transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
	transition-timing-function: cubic-bezier(0.420, 0.000, 1.000, 1.000);
}

@mixin placeholder {
	::-webkit-input-placeholder {@content}
	:-moz-placeholder           {@content}
	::-moz-placeholder          {@content}
	:-ms-input-placeholder      {@content}  
}

%coloranim {
	-webkit-transition: color 150ms linear, background-color 150ms linear, fill 150ms linear, 150ms opacity linear;
	transition: color 150ms linear, background-color 150ms linear, fill 150ms linear, 150ms opacity linear;
}

$retinaDisplay: "(-webkit-min-device-pixel-ratio: 1.25), (min-resolution: 120dpi)";

