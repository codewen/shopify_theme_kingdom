/*-------------------------
    2. STRUCTURE
------------------------*/

/*!
 *  2.1. General
*/

html {

	overflow-y: scroll;
	overflow-x: hidden;
	width: 100%;
	height: 100%;

	&.killflow {
		overflow: hidden;
	}

}

body {

	background-color: $pageBackground;
	//overflow: hidden;
	min-height: 100%;
	font: 500 15px/24px $bodyFont;
	color: $bodyColor;

	&.meta-fixed {
		padding-top: 95px;
	}

	&.killflow-ios {
		position: fixed; 
		left: 0;
		width: 100%;
		height: 100%;	
	}

}

// Column rules

.krown-column-row {
	@extend %clearfix;
}

.krown-column-container {

   margin-left: 4%;
   float: left;
   margin-bottom: 10px;
   position: relative;

	&.span4 {
	   width: 23%;
	}
	&.span4 {
	   width: 30.66%;
	}
	&.span6 {
	   width: 48%;
	}
	&.span8 {
	   width: 65.33%;
	}
	&.span12 {
	   width: 100%;
	}

	&.first {
	   margin-left: 0px;
	}

	&.align-center {
		text-align: center;
	}
	&.align-right {
		text-align: right;
	}

}

.rte {
	margin-top: -10px;
}

.contact-form {
	margin-top: 70px;
}

/*!
 *  2.2. Typography
*/

h1, h2, h3, h4, h5, h6 {
	font-family: $headingsFont;
	color: $headingsColor;
	text-transform: uppercase;
}

h1 {
	font-size: 26px;
	line-height: 32px;
	margin-bottom: 32px;
}
h2 {
	font-size: 22px;
	line-height: 28px;
	margin-bottom: 28px;
}
h3 {
	font-size: 16px;
	line-height: 22px;
	margin-bottom: 22px;
}
h4 {
	font-size: 12px;
	line-height: 18px;
	margin-bottom: 18px;
}

a {

	color: $linksColor;

	&:hover {
		color: desaturate(lighten($linksColor, 15%), 15%);
	}

	&:focus {
		outline: none !important;
	}

	@extend %coloranim;

}

.svg {
	@extend %coloranim;
}

// RTE styling

strong {
	font-weight: bold;
}
em {
	font-style: italic;
}

p, ul, ol {
	margin-bottom: 24px;
}

blockquote {
	margin: 40px 0;
	border-left: 2px solid lighten($blockquoteColor, 50%);
}
blockquote > p, blockquote > div {
	font-size: 22px;
	line-height: 30px;
	font-weight: 500;
	padding: 3px 0 3px 50px;
	color: $blockquoteColor;
}

p img, p iframe {
	margin-bottom: 12px;
}

ul {
	list-style: inside disc;
}
ol {
	list-style: inside decimal;
}
ul ul, ul ol, ol ol, ol ul {
	margin-left: 20px;
}

table {
	width: 100%;
	border: 1px solid $dividersColor;
	text-align: left;
}
table thead h4 {
	font-size: 10px;
	line-height: 18px;
	text-transform: uppercase;
	margin-bottom: 0;
}
table tr {
	border-top: 1px solid $dividersColor;
}
table thead tr {
	border-top: none;
}
table th, table td {
	min-height: 41px;
	padding: 10px 14px 8px;
	border-right: 1px solid $dividersColor;
}

sub, sup {
	font-size: .7em;
	line-height: .7em;
}
sub {
	vertical-align: bottom;
}
sup {
	vertical-align: top;
}


/*!
 *  2.3. Sidebar
*/

#sidebar-holder {
	position: relative;
	float: left;
	width: 290px;
	height: 1px;
}

#sidebar {

	width: 290px;
	background: $sidebarBackground;
	color: $sidebarText;
	padding: 70px 0 0;
	text-align: center;
	position: fixed;
	min-height: 100%;
	display: flex;
	flex-direction: column;
	z-index: 99999;

	&:after {
	    height: 100%;
	    position: absolute;
	    width: 100%;
	    display: block;
	    content: "";
	    background: inherit;
	    left: 0px;
	    z-index: 1;
	    margin-top: 0;
	}

	a {
		color: $sidebarLinks;
		&:hover {
			color: desaturate(lighten($sidebarLinks, 15%), 15%);
		}
	}

	> * {
		z-index: 9;
	}

}

#content-holder {
	@extend #sidebar-holder;
}

#content {
	display: block;
	width: calc(100% - 290px);
	position: fixed;
}

// Logo

#logo {

	a {
		display: inline-block;
		position: relative;
		color: $logoColor !important;
	}

	h1, h3 {
		font-size: 30px;
		line-height: 30px;
		text-transform: uppercase;
		font-weight: 700;
		margin: 0;
	}

	.logo-txt {
		padding: 0 20px;
	}

}

// Meta icons

#meta {

	margin-top: 55px;

	.item {

		display: inline-block;
		margin: 0 7px;
		width: 36px;
		height: 36px;
		position: relative;

		> a {
			position: absolute;
			top: 0;
			left: 0;
			width: 36px;
			height: 36px;
			text-indent: -9999px;
			z-index: 99;
		}

		> .svg {
			fill: $sidebarIconsColor;
			shape-rendering: geometricprecision;
			position: absolute;
			top: 0;
			left: 0;
			.ie8 &:after {
				color: $sidebarIconsColor;
			}
		}
		&:hover > .svg, &.opened > .svg {
			fill: $sidebarIconsColorHover;
		}

		.svg.cart {
			@include scale(.35, top left);
			top: 2px;
			left: 2px;
		}
		.svg.search {
			@include scale(.23, top left);
			top: 8px;
			left: 5px;
		}
		.svg.account {
			@include scale(.23, top left);
			top: 8px;
			left: 5px;
		}
		.svg.hamburger {
			top: -9px;
			left: -12px;
			shape-rendering: crispedges;
		}

	}

	.responsive-menu {
		display: none;
	}

	.count {

		color: $sidebarBackground;
		font: normal 11px/11px $menuFont;
		position: absolute;
		text-align: center;
		top: 16px;
		left: 5px;
		width: 25px;

		.ie8 & {
			display: none;
		}

	}

}

// Menus

#menu {

	margin-top: 55px;
	flex: 1;

	.top-menu {

		font: normal 13px/13px $menuFont;	

		a {
			color: $pagesMenu;
		}

		> li > a {
			display: inline-block;
			padding: 10px 20px;
			position: relative;
		}

		li.submenu {

			position: relative;

			.svg {    
				fill: $pagesMenu;
				position: absolute;
				top: 15px;
				margin-left: 14px;
				right: 0;
			}

			&.opened > .svg {
				transform: rotate(180deg);
			}
			&.selected.opened > .svg {
				fill: $menuSelected;
			}


		}

	}

	.sub-menu {

		font: normal 11px/11px $menuFont;
		margin: 0 0 18px;
		display: none;

		a {
			color: $pagesSubmenu;
			display: inline-block;
			padding: 9px 20px;
		}

	}

	.collections-menu {

		text-transform: uppercase;
		margin-bottom: 40px;
		font: normal 12px/12px $menuFont;
		border-top: 1px solid $menuBorder;

		a {

			width: 100%;
			padding: 19px 20px !important;
			border-bottom: 1px solid $menuBorder;

			&:hover {
				background: $menuBorder;
			}

			.svg {
				top: 22px !important;
				right: auto !important;
			}

		}

		a {
			color: $collectionsMenu;
			.svg {
				fill: $collectionsMenu !important;
			}
		}

		.sub-menu {

			font-size: 11px;
			line-height: 11px;
			margin: 0;

			a {
				color: $collectionsSubmenu;
			}

		}

	}

	a:hover {
		color: $menuRollOver !important;
		.svg {
			fill: $menuRollOver !important;
		}
	}

	li.selected > a {
		font-weight: 700;
		color: $menuSelected !important;
		.svg {
			fill: $menuSelected !important;
		}
	}

	.responsive-close {

		position: absolute;
		top: 0;
		right: 0;
		width: 60px;
		height: 60px;
		z-index: 99999;
		display: none;

		.svg {
			fill: $pagesMenu;
		}
		&:hover .svg {
			fill: $menuRollOver;
		}

	}

	ul {
		list-style: none;
		&.size-0 {
			display: none;
		}
	}
}

// Language switcher

#ly-ls {

	width: 270px;
	margin: 25px auto 0;
	font: normal 13px/13px $menuFont;
	text-transform: uppercase;

	.regular-select-cover {
		margin-bottom: 0;
		width: 200px !important;
	}

	a {
		color: $sidebarText;
		margin-right: 10px;
		&:hover {
			color: $sidebarLinks;
		}
	}

	img {
		position: relative;
		top: 1px;
		left: -1px;
	}

	ul {
		list-style: none;
		li {
			padding: 10px 0;
		}
	}

}

// Footer

#footer {

	font: 12px / 20px $bodyFont;
	font-weight: 600;
	padding: 60px 10px 50px;

	.footer-link:after {
		background: none;
		content: "";
		display: inline-block;
		margin: 0 4px 2px 7px;
		width: 2px;
	}
	.footer-link:last-of-type:after {
		display: none;
	}

	p {
		margin-top: 4px;
		margin-bottom: 14px;
		&:empty {
			display: none;
		}
		&:last-of-type {
			margin-bottom: 0;
		}
	}

	a {
		color: $sidebarLinks;
		&:hover {
			color: desaturate(lighten($sidebarLinks, 15%), 15%);
		}
	}

}

// Social icons

.social-icons {

	display: block;
	margin-bottom: 15px;

	a {
		font-size: 0;
		line-height: 0;
		margin: 0 5px;
	}
	i {
		font-size: 21px;
		line-height: 21px;
		color: $sidebarIconsColor;
		@extend %coloranim;
	}
	a:hover i {
		color: $sidebarIconsColorHover;
	}

	&:empty {
		display: none;
	}

}

// Payment icons

.pay-icons {
	
	margin-top: 50px;

	.svg {

		fill: $sidebarIconsColor;
		@include scale(.8, center center);
		@include opacity(.7);

		* {
			fill: $sidebarIconsColor;
		}

		.ie8 & {
			color: $sidebarIconsColor;
		}

	}

	&:empty {
		display: none;
	}

}

/*!
 *  2.4. Overlays
*/

#main-overlay {

	// General rules

	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background: rgba(255, 255, 255, .8);
	overflow: hidden;
	display: none;
	z-index: 99999;

	.ie8 & {
		filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#DAFFFFFF,endColorstr=#DAFFFFFF) !important; 
	}

	> div {

		@include opacity(0);
		visibility: hidden;
		position: absolute;
		top: -100px;

		.close {
			position: absolute;
		}

	}

	h5 {
		font-size: 26px;
		line-height: 34px;
		text-transform: uppercase;
		margin-bottom: 55px;
	}

	.close {

		width: 60px;
		height: 60px;
		background: #1d1d1d;
		position: fixed;
		top: 0;
		right: 0;
		margin: 0 !important;
		@extend %coloranim;

		.svg {
			fill: #fff;
			.ie8 & {
				color: #fff;
			}
		}

		&:hover {
			background: #313131;
		}

		.ie8 & {
			line-height: 38px;
		}

	}

	.simple-overlay-box {

		margin: auto;
		width: 670px;
		background: #fff;
		padding: 100px 90px 90px;
		display: block;
		position: relative;
		z-index: 9999;

	}

	// Search form

	form {
		position: relative;
		width: 490px;
		margin: auto;
	}

	#search-overlay input {

		height: 76px;
		width: 490px;
		font-size: 18px;
		line-height: 18px;
		color: $formsLabel;
		text-weight: 500;
		background: $formsBack;
		border: 2px solid lighten(invert($formsBack), 90%);
		border-radius: $formsStyle;
		padding: 0 80px 0 30px;

		.ie8 & {
			padding-top: 28px !important;	
		}

	}

	button[type="submit"] {
		background: none !important;
		border: medium none !important;
		height: 25px;
		position: absolute;
		right: 30px;
		top: 26px;
		width: 30px;
		cursor: pointer;
		padding: 0;
	}

	.svg.search {

		fill: #1d1d1d;
		@include scale(.25, top left);

		.ie8 & {
			margin-top: -20px;
		}

	}

	// Sharing icons

	#share-overlay .simple-overlay-box {
		padding-left: 0;
		padding-right: 0;
	}

	#share-overlay a {

		color: $linksColor;
		font-size: 58px;
		line-height: 58px;
		margin: 10px 24px;
		display: inline-block;

		&:hover {
			color: #1d1d1d;
		}

	}

	// Cart actions

	#cart-overlay {

		text-align: center;

		a {
			font-size: 15px;
