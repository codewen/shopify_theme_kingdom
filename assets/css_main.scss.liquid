/*!
 *  5.2. Landscape tablets
*/

@media all and (max-width: 900px) {

	#size-mobile {
		display: block;
	}

	// Sidebar 

	#sidebar-holder {
		height: auto !important;
		width: 100%;
	}

	#sidebar {

		width: 100%;
		top: 0 !important;
		padding: 30px 0 10px;
		position: relative;

		&:after {
			display: none;
		}

		body.meta-fixed & {
			padding-top: 0;
		}

	}

	#meta {

		margin-top: 20px;
		
		.responsive-menu {
			display: inline-block;
		}

		body.meta-fixed & {
			position: fixed;
			width: 100%;
			height: 65px;
			left: 0;
			top: 0;
			background: $sidebarBackground;
			margin: 0;
			padding-top: 12px;
			z-index: 99999;
		}

	}

	#options {
		margin: 0 0 40px;
	}

	// Menu

	#menu {

		position: fixed;
		@include alphaback(darken($sidebarBackground, 3%), .95);
		z-index: 99999;
		padding: 0;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		margin: 0;
		display: block;
		@include opacity(0);
		visibility: hidden;

		> div {    
			display: block;
			top: 0;
			position: fixed;
			width: 100%;
			height: 100%;
			padding: 50px 0 20px;
			overflow: auto;
		}

		.collections-menu {

			margin-bottom: 50px;
			border-top: none;

			a {

				width: auto;
				border-bottom: none;
				font-size: 20px;
				line-height: 20px;
				font-weight: 700;
				padding: 15px 20px !important;

				&:hover {
					background: none;
				}

				.svg {

					top: 24px !important;
					right: 0 !important;

					@media #{$retinaDisplay} { 
						top: 22px !important;
					}

				}

			}

			.sub-menu a {

				font-size: 16px;
				line-height: 16px;
				font-weight: 400;
			}

		}

		.top-menu {

			font-size: 14px;
			line-height: 14px;

			.svg {
				top: 16px !important;
			}

		}

		.sub-menu, .option, .simple-select-inner {
			font-size: 14px;
			line-height: 14px;
		}

	}

	// Content

	#content-holder {
		@extend #sidebar-holder;
	}

	#content {
		width: 100%;
		position: relative;
		top: 0 !important;
	}

	// Footer

	#footer {
		background: $sidebarBackground;
		color: $sidebarText;
		padding: 60px 15px 55px;
		text-align: center;
		position: relative;
		float: left;
		width: 100%;
		&.loading {
			margin-top: 300px;
		}
	}

	// Products grid

	.isotope-products:after {
		left: 50%;
	}

	.grid-item {

		&.one .caption, &.three .caption {

			position: relative;
			opacity: 1 !important;
			visibility: visible !important;
			height: auto;
			bottom: 0px;
			padding: 10px 30px;
			height: 70px;
			display: table;

			&:after {
				bottom: 100%;
				left: 50%;
				border: solid transparent;
				content: " ";
				height: 0;
				width: 0;
				position: absolute;
				pointer-events: none;
				border-width: 10px;
				margin-left: -10px;
			}

			> div {
				display: table-cell;
				vertical-align: middle;
			}

			.collection {
				display: none;
			}

			h2 {
				font-size: 12px;
				line-height: 16px;
				margin: 0;
				width: auto;
			}

			.info {
				font-size: 15px;
				line-height: 15px;
				margin-top: 0;
			}

			.bottom {
				font-size: 12px;
				bottom: 0;
				right: 0;
				position: relative;
				text-align: right;
				strong {
					@extend .info;
				}
			}

		}

		&.three .caption {

			height: auto;
			padding: 20px;

			> div > div {
				display: block;
			}

			.price {
				top: 0 !important;
			}

			h2 {

				@extend .price;
				margin-bottom: 7px;

				&:after {
					display: none;
				}

			}

		}

	}

	.draw-buttons.nav {

		width: 100%;
		left: 0;
		right: auto;
		background: $pageBackground;
		height: 60px;
		text-align: center;
		padding-left: 64px;
		
		&.three {
			padding-left: 0;
			padding-right: 0;
		}
		&.last-nav-mobile {
			padding-left: 0;
			padding-right: 64px;
		}

		.no-touch &:hover {
			width: 100% !important;
		}

		.holder {

			position: static;
			width: auto;

			> span {
				float: none;
				display: inline-block;
				background: transparent !important;
				position: relative;
				left: auto !important;
			}

		}

		.back {
			display: block !important;
		}
		.front {
			display: none !important;
		}

		.svg {
			fill: $headingsColor;
		}
		& span:hover .svg {
			fill: $bodyColor;
		}

	}

	.product-gallery {
		top: 60px !important;
		.move-top & {
			top: 0 !important;
		}
	}
	.product-content {
		@extend .product-gallery;
		padding-bottom: 140px;
		min-width: 300px;
		.move-top & {
			padding-bottom: 80px;
		}
	}

	// Blog

	.blog-grid {

		&.cols-2, &.cols-3 {
			margin-left: -30px;
			.post {
				padding: 0 30px;
			}
		}

		&.cols-3 .post {
			width: 50%;
		}

	}

	// Home Slider

	#home-slider {

		min-height: 300px;

		.slide {
			position: relative;
		}
		.slide-img {

			@extend .slide;
			background-image: none !important;

			.slide-img-holder {
				position: relative;
			}
			img {
				position: relative;
				display: block;
				width: 100% !important;
				max-width: none;
				height: auto !important;
		    object-fit: contain;
		    font-family: "object-fit: contain";
				@include opacity(1);
			}

		}

		.slide-caption {

			width: 280px;

			.label.a {
				font-size: 14px;
				line-height: 18px;
				margin-bottom: 20px;
				text-transform: none;
			}
			.label.b {
				font-size: 20px;
				line-height: 24px;
				font-weight: 700;
				margin-bottom: 20px;
			}
			.label.c {
				margin-bottom: 30px;
			}

		}

		.draw-buttons {
			display: none !important;
		}

	}

	.swiper-pagination {

		display: block;
		width: 100%;
		height: 60px;
		background: #fff;
		text-align: center;
		padding-top: 16px;
		position: relative;
		z-index: 99;

		&:after {
			bottom: 100%;
			left: 50%;
			border: solid transparent;
			content: " ";
			height: 0;
			width: 0;
			position: absolute;
			pointer-events: none;
			border-color: rgba(#fff, 0);
			border-bottom-color: #fff;
			border-width: 10px;
			margin-left: -10px;
		}

		span {

			width: 8px;
			height: 8px;
			border-radius: 4px;
			display: inline-block;
			margin-left: 5px;
			background: #ccc;
			cursor: pointer;

			&:first-child {
				margin-left: 0;
			}
			&.swiper-active-switch, &.swiper-pagination-bullet-active {
				background: #1d1d1d;
				cursor: default;
			}

		}
	}

	// Other

	.krown-column-container {
		width: 100% !important;
		margin-left: 0;
	}

	#page-content {
		padding: 85px 11%;
	}

