/*!
 *  3.7. Article
*/

.post-navigation span:first-child {
	margin-right: 40px;
}

// Comments

#comments {

	margin-top: 100px;
	display: block;

	.note.form-error {
		margin-top: -30px;
	}
	.note.no-comments {
		margin-bottom: -30px;
	}	

	ul {
		list-style: none;
	}

}

.note {    

	font-size: 12px;
	margin: 30px 0;

	ul {
		list-style: none;
	}

}

.comment {

	position: relative;
	padding-top: 9px;
	margin-bottom: 55px;

	.avatar {
		position: absolute;
		top: 0;
		left: 0;
		width: 60px;
		height: 60px;
	}

	img {
		margin: 0 !important;
	}

	.content {
		padding-left: 100px;
	}

	h5 {
		font: 600 15px/15px $bodyFont;
		color: $headingsColor;
		margin-bottom: 6px;
	}

	time {
		font-size: 13px;
		line-height: 13px;
		color: $bodyExtraColor;
		font-weight: 500;
	}

	.text {
		margin-top: 24px;
	}

}

#comments-form {
	margin-top: 130px;
}
#comments h3, #comments-form h3 {
	margin-bottom: 50px;
}

.hidden-label {
	display: none;
}

/*!
 *  3.8. Search
*/

.template-search form.search-bar {
	width: 360px;
}

// Search item

.search-item {

	list-style: none;
	position: relative;
	border-top: 1px solid #ececec;

	.search-img {
		@extend %small-product-img;
		position: absolute;
		left: 0;
	}

	&.product {
		padding: 50px 0;
	}
	&.article {
		padding: 45px 0 43px;
	}

	.product-content {
		float: left;
		padding-left: 160px;
		min-height: 114px;
	}
	.comparePrice {
		color: $comparePrice;
		text-decoration: line-through;
		font-size: 14px;
	}

	h3, .excerpt {
		margin-bottom: 10px;
	}

}

/*-------------------------
    4. IE8 TWEAKS
*------------------------*/

.ie8 {

	.page-title, .post-header, #breadcrumb, #menu, #footer {
		display: block;
	}

	.svg {

		display: block;
		text-indent: 0 !important;

		&:after {
			font-family: "FontAwesome";
			font-size: 24px;
			line-height: 60px;
		}

		&.cart:after {
			content: "\f07a";
		}
		&.search:after {
			content: "\f002";
		}
		&.account:after {
			content: "\f007";
		}
		&.close:after {
			content: "\f00d";
		}
		&.reload:after {
			content: "\f021";
		}
		&.right:after {
			content: "\f105";
		}
		&.left:after {
			content: "\f104";
		}
		&.grid:after {
			content: "\f00a";
		}
		&.arrow:after {
			content: "\f0d7";
			font-size: 11px;
			line-height: 11px;
		}
		&.plus:after {
			content: "\f067";
		}
		&.minus:after {
			content: "\f068";
		}
		&.hamburger:after {
			content: "\f0c9";
		}
		&.amex:after {
			content: "\f1f3";
		}
		&.discover:after {
			content: "\f1f2";
		}
		&.google:after {
			content: "\f1ee";
		}
		&.maestro:after {
			content: "\f1f1";
		}
		&.mastercard:after {
			content: "\f1f1";
		}
		&.paypal:after {
			content: "\f1f4";
		}
		&.visa:after {
			content: "\f1f0";
		}
		&.bitcoint:after {
			content: "\f15a";
		}

	}

	#menu .svg.arrow {
		position: relative;
		top: 8px !important;
		display: inline;
	}
	#menu .collections-menu .svg.arrow {
		top: 18px !important;
	}

	#meta {
		.svg {
			cursor: pointer;
			&:after {
				line-height: 20px;
			}
			&.cart {
				top: 7px;
			}
		}
	}

}

