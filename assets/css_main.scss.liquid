			pointer-events: none;
		}

	}

	.contents {

		min-height: 200px;
		padding: 60px 30px 0;

	   	> div {
	   		display: none;
	   		&.opened, &:first-child {
	   			display: block;
	   		}
		}

	}

}

/*-------------------------
    3. TEMPLATES
*------------------------*/

/*!
 *  3.1. Index
*/

// Fullscreen slideshow

.preloader {
	position: absolute;
	width: 66px;
	height: 66px;
	top: 50%;
	left: 50%;
  z-index: 9;
  transform: translate3d(-50%, -50%, 0);
  pointer-events: none;
	@include transition(opacity, .2s, ease);
	.loaded & {
		opacity: 0;
		visibility: hidden;
	}
	.template-collection & {
		position: fixed;
		@media all and (min-width: 901px) {
			margin-left: 145px;
		}
	}
	.template-collection .infobar + .isotope-products .preloader {
		margin-top: 30px;
	}
	.template-collection .collection-data + .isotope-products .preloader, .no-loader & {
		display: none;
	}

}

.spinner {

	width: 32px;
  height: 32px;
  
  border: 2px solid darken($pageBackground, 20%);
  border-top: 3px solid $linksColor;
  border-radius: 100%;
  
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right: 0;
  margin: auto;
  
  animation: spin 1s infinite linear;

}

@keyframes spin {
  from{
      transform: rotate(0deg);
  }to{
      transform: rotate(360deg);
  }
}



/* Typing Loader ND */

#home-slider {

	position: relative;
	top: 0;
	left: 0;
	width: 100%;
	height: 100vh;
	z-index: 89;

	.swiper-slide {
		background-color: $pageBackground;
	}

	&.disabled {
		pointer-events: none;
	}

	%slide {
		width: 100%;
		height: 100%;
		position: absolute;
		top: 0;
		left: 0;
		z-index: 1;
	}

	.slide {
		width: 100%;
		height: 100%;
	}

	.slide-img {

		@extend %slide;
		@include bgimg();
		@include opacity(0);
		z-index: 9;

		&.onboarding-slide:before {
	    position: absolute;
	    content: "";
	    display: block;
	    top: 0;
	    left: 0;
	    width: 100%;
	    height: 100%;
	    z-index: 9;
	    background: rgba(255,255,255,.9);
	    background: linear-gradient(rgba(255,255,255,.9), rgba(235,235,235,.9));
		}

		.slide-img-holder, img {
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
	    object-fit: cover;
	    font-family: "object-fit: cover";
		}

	}

	.slide-url {
		@extend %slide;
		z-index: 9;
		cursor: pointer;
		pointer-events: all;
	}

	.slide-caption {

		@extend %slide;
		z-index: 11;
		width: 360px;

		&.left {
			left: 12%;
		}
		&.center {
			left: 50%;
			margin-left: -180px;
			text-align: center;
			.label {
				text-align: center;
				&.d {
					float: none;
				}
			}
		}
		&.right {
			right: 12%;
			left: auto;
		}

		.label {
			display: block;
			position: relative;
			text-align: left;
			top: 60px;
			color: #1d1d1d;
			@include opacity(0);
			@include prefix(backface-visibility, visible);
			visibility: hidden;
		}
		.label.a {
			font: {{ settings.typography_label1_style }} {{ settings.typography_label1_size }}px {{ settings.typography_label1 }};
			line-height: calculate-line-height({{ settings.typography_label1_size }});
			margin-bottom: 37px;
		}
		.label.b {
			font: {{ settings.typography_label2_style }} {{ settings.typography_label2_size }}px {{ settings.typography_label2 }};
			line-height: calculate-line-height({{ settings.typography_label2_size }});
			margin-bottom: 20px;
		}
		.label.c {
			font: {{ settings.typography_label3_style }} {{ settings.typography_label3_size }}px {{ settings.typography_label3 }};
			line-height: calculate-line-height({{ settings.typography_label3_size }});
			margin-bottom: 50px;
		}
		.label.d {
			display: inline-block;
			float: left;
			pointer-events: all;
			span {
				font: normal 15px/13px Arial, Helvetica, sans-serif;
				margin-left: 10px;
			}
		}
		.label:last-child {
			margin-bottom: 0;
		}

		&.dark .label {
			color: #f7f7f7;
		}

	}
	
	.draw-buttons {
		display: none;
	}

	video {

	  position: absolute;
	  top: 50%;
	  left: 50%;
	  min-width: 100%;
	  min-height: 100%;
	  width: auto;
	  height: auto;
	  z-index: 9;
	  margin: 0;
	  max-width: none;
	  vertical-align: middle;
	  -ms-transform: translate(-50%, -50%);
	  -webkit-transform: translate(-50%, -50%);
	  transform: translate(-50%, -50%); 
	  opacity: 0;
	  transition: opacity 300ms linear;

	  &.active {
	  	opacity: 1;
	  	+ .slide-img-holder {
	  		display: none;
	  	}
	  }

		@media all and (max-width: 640px) {
			position: static;
			min-width: 0;
			min-height: 0;
		  -ms-transform: none;
		  -webkit-transform: none;
		  transform: none; 
		  width: 100%;
		}

	}

	&.disabled + .swiper-pagination {
		display: none !important;
	}

}
