		display: inline-block;
		font-weight: 600;
		font-size: 16px;
		line-height: 30px;
		width: 30px;
		height: 30px;
		text-align: center;
		margin-right: 15px;

		&.active {
			color: $headingsColor;
			cursor: default;
		}

		a:hover {
			color: lighten($headingsColor, 20%);
		}

		&.arrow {
			font-family: Arial, Helvetica, sans-serif;
			font-size: 18px;
			&.prev {
				margin-left: 7px;
			}
		}

	}

	.template-search & {
		margin-top: 45px;
	}

