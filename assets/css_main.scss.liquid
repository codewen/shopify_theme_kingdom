/*!
 *  2.5. Forms
*/

// Custom select boxes

.regular-select-cover {

	color: $formsLabel;
	height: 56px;
	width: 100% !important;
	font-size: 15px;
	line-height: 15px;
	cursor: pointer;
	background: $formsBack;
	border: 2px solid $formsBorder;
	text-align: left;
	border-radius: $formsStyle;
	margin-bottom: 20px;

	svg {
		position: absolute;
		top: 25px;
		right: 17px;
		fill: $formsLabel;
	}

	select {
		height: 56px !important;
		width: 100%;
		cursor: pointer;
		-webkit-appearance: menulist-button;
	}

	option {
		padding: 2px 0;
	}

}
.regular-select-inner {
	height: 56px;
	padding: 20px 0px 0px 20px;
	cursor: pointer;
}

.simple-select-cover {

	font: normal 13px/13px $headingsFont;
	cursor: pointer;
	background: transparent;

	.svg {

		position: absolute;
		top: 4px;
		right: 0;

		.ie8 & {
			top: -1px;
		}

	}

	select {
		height: 100%;
		width: 110%;
		left: -5px !important;
		cursor: pointer;
		-webkit-appearance: menulist-button;
	}

	option {
		padding: 2px 5px;
	}

}
.simple-select-inner {
	cursor: pointer;
	padding-right: 24px;
}

// Basic form styling

input, textarea {

	background: $formsBack;
	border: 2px solid $formsBorder;
	color: $formsLabel !important;
	height: 56px;
	font: 600 15px/15px $bodyFont;
	border-radius: $formsStyle;
	box-shadow: none;
	margin-bottom: 20px;
	padding: 0 18px;
	width: 100%;
	-webkit-appearance: none;
	resize: none;

	&[type="number"], &.quantity-selector, &.count {
		width: 60px;
		text-align: center;
		margin-right: 20px;
		padding: 0 6px;
		-moz-appearance: textfield;
		-webkit-appearance: none;
	}

	&::-webkit-outer-spin-button, &::-webkit-inner-spin-button {
		-webkit-appearance: none;
	}

	&[type="checkbox"], &[type="radio"] {
		width: auto;
		height: auto;
		padding: 0;
		border: none;
	}
	&[type="checkbox"] {
		-webkit-appearance: checkbox;
	}
	&[type="radio"] {
		-webkit-appearance: radio;
	}

	&[type="search"] {
		-webkit-appearance: none;
	}

	.ie8 &[type="text"], .ie8 &[type="password"] {
		padding-top: 20px !important;	
	}

	::-ms-clear {
		display: none;
	}

}

@include placeholder {
	color: $formsLabel !important;
	opacity: .7;
}

textarea {
	padding: 20px 20px;
	height: 200px;
}
input[type="submit"], button, .button {

	background: $mainButtons;
	font: bold 13px/13px $headingsFont;
	text-transform: uppercase;
	border-radius: $formsStyle;
	height: 56px;
	color: set-button-text-color($mainButtons) !important;
	box-shadow: none;
	border: none;
	cursor: pointer;
	padding: 0 25px;
	margin-bottom: 20px;
	display: inline-block;
	width: auto;
	@extend %coloranim;
	-webkit-appearance: none;

	&:hover {
		background: desaturate(lighten($mainButtons, 7%), 10%);
	}

	&.dark {
		background: $secondaryButtons;
		color: set-button-text-color($secondaryButtons) !important;
		&:hover {
			background: desaturate(lighten($secondaryButtons, 15%), 15%);
		}
	}

	.svg {
		fill: #fff;
		margin-top: 2px;
	}

	&.text-link {
		background: none;
		padding: 0;
		border: none;
		color: $mainButtons !important;
	}

}
a.button {
	padding-top: 21px;
}

/*!
 *  2.6. Other
*/

// Tabs

.krown-tabs {

	.titles {
   		border-bottom: 3px solid darken( $pageBackground, 5%);
   		@extend %clearfix;
	}

	.titles h5 {

		display: block;
		margin: auto;
		font-size: 12px;
		line-height: 60px;
		font-weight: 400;
		color: $headingsColor;
		text-transform: uppercase;
		line-height: 16px;
		padding: 2px 30px 0;
		float: left;
