/*!
 *  3.2. Collections
*/

// Top bar

.infobar {

	background: $pageBackground;
	width: 100%;
	height: 70px;
	line-height: 70px;
	padding: 0 35px;
	overflow: hidden;

	.share-link {

		float: right;
		font-size: 13px;
		margin-top: 0;

		.svg {
			top: 25px;
		}

	}

	strong {
		float: left;
		text-transform: uppercase;
		margin-right: 70px;
		font-family: $headingsFont;
		font-size: 13px;
		font-weight: 400;
		color: $collectionsTitle;
		padding-top: 2px;
	}

	+ .isotope-products.border-true.normal {
		margin-top: -4px;
	}
	+ .isotope-products.border-true.large {
		margin-top: -12px;
	}

}

#options {

	float: left;

	.option {

		font-size: 13px;
		color: $collectionsOption;
		display: inline-block;
		margin-right: 40px;

		label {
			margin-right: 5px;
		}
		select {
			@include opacity(0);
			&.styled {
				@include opacity(1);
			}
		}

		.simple-select-cover {
			top: -1px !important;
		}
		.simple-select-inner {
			color: $collectionsOption;
			font-weight: 700;
			font-size: 12px;
			line-height: 12px;
		}

		.svg {
			fill: rgba($collectionsOption, .7);
		}

	}

}

// Collection data

.collection-data {

	position: relative;
	clear: left;

	> img {
		width: 65%;
		height: auto;
		display: block;
	}

	> div {

		background: lighten($pageBackground, 10%);
		height: 100%;
		width: calc(35% + 20px);
		position: absolute;
		top: 0;
		right: -20px;
		padding: 30px 50px 0;

		.table-display > div {
			text-align: left;
			padding-bottom: 30px;
		}

	}

}

.template-index #page-content .home-nomargin:not(:first-child) {
	.isotope-products:after {
		display: none !important;
	}
}

// Grid item

.isotope-products {

	min-height: 100vh;
	width: 100%;

	&.loaded {
		min-height: 0;
	}

	&.border-true.tiny {
		width: calc(100% - 2px);
		left: 2px;
		top: 2px;
		margin-bottom: 2px;
		.grid-item {
			padding: 2px;
			@media all and (min-width: 901px) {
				&.one .caption, &.three .caption {
					width: calc(100% - 2px);
					height: calc(100% - 2px);
					left: 1px;
					top: 1px;
				}
			}
			&.two:before {
				width: calc(100% - 2px);
				height: calc(100% - 2px);
				left: 1px;
				top: 1px;
			}
			.add {
				margin-right: 2px;
			}
		}
	}
	&.border-true.normal {
		width: calc(100% - 8px);
		left: 4px;
		top: 4px;
		margin-bottom: 8px;
		.grid-item {
			padding: 4px;
			@media all and (min-width: 901px) {
				&.one .caption, &.three .caption {
					width: calc(100% - 8px);
					height: calc(100% - 8px);
					left: 4px;
					top: 4px;
				}
			}
			&.two:before {
				width: calc(100% - 8px);
				height: calc(100% - 8px);
				left: 4px;
				top: 4px;
			}
			.add {
				margin-right: 4px;
			}
		}
	}
	&.border-true.large {
		width: calc(100% - 16px);
		left: 8px;
		top: 8px;
		margin-bottom: 16px;
		.grid-item {
			padding: 8px;
			@media all and (min-width: 901px) {
				&.one .caption, &.three .caption {
					width: calc(100% - 16px);
					height: calc(100% - 16px);
					left: 8px;
					top: 8px;
				}
			}
			&.two:before {
				width: calc(100% - 16px);
				height: calc(100% - 16px);
				left: 8px;
				top: 8px;
			}
			.add {
				margin-right: 8px;
			}
		}
	}

}

.grid-item {
	
	@media all and (min-width: 2560px) {
		width: 16.666%;
	}
	@media all and (max-width: 2560px) {
		width: 20%;
	}
	@media all and (max-width: 2080px) {
		width: 25%;
	}
	@media all and (max-width: 1640px) {
		width: 33.33%;
	}
	@media all and (max-width: 1180px) {
		width: 50%;
	}
	@media all and (max-width: 460px) {
		width: 100% !important;
	}

	.featured-collections-section .collections-list[data-blocks-size="1"] & {
		width: 100%;
	}
	.featured-collections-section .collections-list[data-blocks-size="2"] & {
		width: 50%;
	}
	.featured-collections-section .collections-list[data-blocks-size="3"] & {
		width: 33.33%;
	}


	float: left;
	position: relative;
	overflow: hidden;
	pointer-events: none;
	@include opacity(0);
	@include transition(opacity, .3s, ease);

	&.loaded {
		pointer-events: all;
		@include opacity(1);
	}

	.grid-item-image {
		width: 100%;
		height: 0;
		position: relative;
	  &:after {
		  display: block;
		  content: "";
		  position: absolute;
		  top: 0;
		  left: 0;
		  width: 100%;
		  height: 100%;
		  background: #000;
		  opacity: .05;
		}
	}
	img {
		z-index: 8;
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
    object-fit: cover;
    font-family: "object-fit: cover";
	}

	// First caption style

	&.one .caption {

		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;

		padding: 10% 16% 0 13%;
		text-transform: uppercase;
		z-index: 9;
		font-family: $headingsFont;

		.top {
			position: relative;
		}

		.collection {
			display: block;
			font-size: 12px;
			line-height: 12px;
			opacity: .5;
			margin: 5px 0 15px;
		}

		h2 {
			font-size: 18px;
			line-height: 24px;
			color: #1d1d1d;
			width: 75%;
		}

		.bottom {
			position: absolute;
			bottom: 15%;
			color: #1d1d1d;
			font-size: 19px;
			line-height: 19px;
		}

		.info {

			font-size: 16px;
			line-height: 16px;
			color: #878787;
			text-decoration: line-through;
			display: block;
			margin-top: 5px;

			.collections-list & {
				text-decoration: none;
				text-transform: none;
			}

		}

	}

	.collection span:not(:last-child):after {
		content: ", ";
	}

	// Second caption style

	&.two {

		&:before {

			content: "";
			display: block;
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			z-index: 9;
			background: #000;
			@include opacity(0);
			@include transition(opacity, .2s, ease);

			.ie8 & {
				display: none;
			}

		}

		.caption {

			width: 100%;
			height: 160px;
			text-transform: uppercase;
			z-index: 9;
			font-family: $headingsFont;
			position: relative;
			text-align: center;
			display: table;
			color: #1d1d1d;

			> div {
				display: table-cell;
				vertical-align: middle;
			}

			h2 {
				font-size: 12px;
				line-height: 17px;
				padding: 0 20%;
				margin-bottom: 12px;
				@extend %coloranim;
			}

			.price {
				font-size: 22px;
				line-height: 22px;
				font-weight: 700;
				@extend %coloranim;
			}

			.info {
				font-size: 18px;
				line-height: 18px;
				opacity: .5;
				font-weight: 400;
				text-decoration: line-through;
				display: inline-block;
				@extend %coloranim;
			}

			.collections-list & {
				display: none;
			}

		}

		&:hover {
			
			&:before {
				@include opacity(.2);
			}
			.caption h2, .caption .price, .caption .info {
				color: #fff;
			}
			
		}

		.add {
			
			width: 62px;
			height: 62px;;
			border-radius: 31px;
			top: 30px;
			right: 30px !important;
			text-align: center;
			line-height: 12px;
			padding: 0 5px;
			display: table;

			&:after {
				display: none;
			}

			span {
				display: table-cell;
				vertical-align: middle;
			}
			
		}

	}

	// Third caption style

	&.three .caption {

		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		
		padding: 0 100px;
		text-transform: uppercase;
		text-align: center;
		z-index: 9;
		font-family: $headingsFont;

		> div {
			display: table;
			width: 100%;
			height: 100%;
		} 
		> div > div {
			display: table-cell;
			vertical-align: middle;
		}

		.collection {
			opacity: .5;
		}

		h2 {

			font-size: 18px;
			line-height: 24px;
			color: #1d1d1d;
			margin-bottom: 27px;
			position: relative;

			&:after {
				content: "";
				display: block;
				width: 30px;
				height: 2px;
				background: #ff0000;
				margin: 25px auto 0;
			}

		}

		.price {

			position: relative;

			strong {
				font-size: 18px;
				line-height: 18px;
				color: #1d1d1d;
			}

		}

		.info {

			font-size: 13px;
			line-height: 13px;
			color: #6a6a6a;
			text-decoration: line-through;
			display: block;
			margin-top: 2px;

			.collections-list & {
				text-decoration: none;
				text-transform: none;
			}

		}

	}

	// Fourth caption style

	&.four {

		transition: opacity 300ms, transform 170ms cubic-bezier(0.42, 0, 0.54, 1.01), box-shadow 150ms linear;
    z-index: 999;

		.no-touch &:hover {
	    transform: translate3d(20px, 20px, 0);
	    box-shadow: 10px 10px 12px rgba(150, 150, 150, .05);
		}

		.caption {

			width: 100%;
			height: 100px;
			padding: 0 40px;
			position: relative;
			display: flex;
	    justify-content: space-between;
	    align-items: center;

			h2 {
				font-size: 13px;
				line-height: 15px;
				font-weight: 600;	
				margin-bottom: 0;
			}

			h5 {

				text-transform: none;
				font-size: 13px;
				line-height: 15px;
				opacity: .5;
				margin-top: 7px;

				span {
					font-family: $quotesFont;
					font-style: italic;
					padding-right: 5px;
				}

			}

			.price {

				font-size: 13px;
				line-height: 15px;
				font-family: $headingsFont;

				strong {
					font-weight: 500;
					position: relative;
				}

				.info {    
					opacity: .5;
	    		text-decoration: line-through;
	    		margin-top: 7px;
	    		display: block;
				}

			}

		}

		.add.sale {
			background-color: #9e9e9e;
			&:after {
				border-color: #9e9e9e;
			}
		}

	}

	// Dark caption (all styles)

	&.dark .caption {

		h2, h5, .bottom, .price strong, .info, .collection {
			color: #f7f7f7 !important;
		}

	}

	&.light .caption {

		h2, h5, .bottom, .price strong, .info, .collection {
			color: #222 !important;
		}

	}

	// Stickers

	.add {

		position: absolute;
		top: 40px;
		right: 0;
		color: #fff;
		text-transform: uppercase;
		font: normal 11px/11px $headingsFont;
		display: block;
		padding: 12px 15px 13px 12px;
		width: auto;
		height: auto;
		z-index: 10;
		@extend %coloranim;

		&:after {
			display: block;
			position: absolute;
			left: -17px;
			top: 0;
			content: "";
			border-width: 18px 0px 18px 18px;
			border-style: solid;
			border-left-color: transparent !important;;
		}

		&.sale {
			background-color: #f44444;
		}
		&.sold {
			background-color: #6a6a6a;
		}
		&.featured {
			background-color: #f48a43;
		}
		&.sale:after {
			border-color: #f44444;
		}
		&.sold:after {
			border-color: #6a6a6a;
		}
		&.featured:after {
			border-color: #f48a43;
		}

	}

	// Hovering (style #1)

	&.one {

		.caption {

			@include opacity(0);
			@include transition(opacity, .3s, ease);

			.ie8 & {
				visibility: visible;
				display: none;
			}

		}

		&:hover .caption {

			@include opacity(1);

			.ie8 & {
				display: block;
			}

		}

	}

	// Hovering (style #3)

	&.three {

		.caption {

			@include opacity(0);
			@include transition(opacity, .3s, ease);

			.ie8 & {
				display: none;
			}

			h2, .price {
				@include transition(top, .3s, ease);
			}
			h2 {
				top: 60px;
			}
			.price {
				top: 30px;
			}

		}

		&:hover .caption {

			@include opacity(1);

			.ie8 & {
				display: block;
			}

			h2 {
				top: 0;
			}
			.price {
				top: 0; 
			}

		}

	}

}

.add.sold + .add.sale {
	display: none;
} 

/*!
 *  3.3. Product
*/

.shopify-section.mount-product {
	position: relative;
}
.product-page {
	position: relative;
	@media all and (min-width: 1181px) {
		.template-index & {
			padding: 5%;
			min-height: 90vh;
		}
	}
}

// Content area

.product-content {

	width: 25%;
	min-width: 330px;
	height: 100%;
	background: $pageBackground;
	position: relative;
	display: inline-block;
	float: left;
	padding: 65px 50px;

	@media all and (min-width: 1181px) {
		.template-index & {
			padding: calc(5% + 10px) 100px 5% 5%;
			background: transparent;
	    min-width: 520px;
			@media all and (max-width: 1626px) {
	    	min-width: 460px;
			}
			@media all and (max-width: 1352px) {
	    	min-width: 400px;
				padding: calc(3% + 10px) 80px 3% 3%;
			}
			.section-title {
				font-size: 11px;
				text-transform: uppercase;
				font-family: $headingsFont;
				margin-bottom: 30px;
				display: block;
			}
			&:after {
				display: none;
			}
		}
	}

	&:after {
		@extend #sidebar:after;
	}

	// Meta information

	.product-price {

		font: bold 30px/30px $headingsFont;
		color: $headingsColor;
		border-top: 1px solid $dividersColor;
		border-bottom: 1px solid $dividersColor;
		padding: 15px 0;
		margin: 35px 0;
		display: block;

		span.info {
			color: $comparePrice;
			font-size: 18px;
			line-height: 18px;
			font-weight: normal;
			text-transform: uppercase;
		}

		.comparePrice {
			text-decoration: line-through;
		}

	}

	// Description

	div[itemprop="description"] {

		> div {
			overflow: hidden;
		}

		.more {
			margin-top: 18px;
			display: inline-block;
		}

	}

	.more, .share-link {
		font-size: 13px;
		line-height: 13px;
	}

	// Cart form

	.add-to-cart {
		margin-top: 42px;
	}

	.addToCart {
		width: 100%;
		&.disabled {
			width: 100% !important;
			background: $bodyExtraColor;
			cursor: normal;
			pointer-events: none;
			margin-bottom: 21px;
		}
	}


	.product-color-variant{
		display:block;
		li{
			border-radius:50%;
			display:inline-block;
			width:30px;
			height:30px;
			border:3px solid white;
		}

	}

	.quantity-selector + .addToCart {
		width: calc(100% - 84px);
	}

	select:not(.style) {
		display: none;
	}

}

.variant-quantity {
	display: inline-block;
}

// Sharing links

.share-link {

	display: inline-block;
	position: relative;
	margin-top: 20px;
	padding-left: 30px;

	.ie8 & {
		padding-left: 0;
	}

	.svg {
		fill: $bodyExtraColor;
		position: absolute;
		top: 3px;
		left: 0;
	}

	.template-product & {
		padding-top: 6px;
	}

}

// Breadcrumbs

#breadcrumb {
	
	text-transform: uppercase;
	font: normal 11px/18px $headingsFont;
	color: $bodyExtraColor;
	margin-bottom: 35px;

	span[aria-hidden="true"] {
		color: rgba($bodyExtraColor, .5);
		margin: 0 5px;
	}
	
	a {
		font-size: 11px;
		line-height: 18px;
	}

}

.product-vendor {
	font-size: 13px;
	line-height: 19px;
	margin-top: 20px;
	font-weight: 500;
}

// Gallery

.product-gallery {

	.template-product & {
		width: calc(75% - 220px);
		height: 100%;
		position: fixed;
		top: 0;
		right: 0;
		@media all and (max-width: 1626px) {
			& {
				width: calc(100% - 620px);
			}
		}
	}

	@media all and (min-width: 1181px) {

		.template-index & {
	    width: 75%;
	    height: 90vh;
	    top: 0;
	    position: sticky;
	    -webkit-position: sticky;
	    float: left;
	    overflow: hidden;
	    max-width: calc(100% - 520px);
	    padding: 5% 5% 5% 0;
	    display: block;
			@media all and (max-width: 1626px) {
	    	max-width: calc(100% - 460px);
			}
			@media all and (max-width: 1352px) {
	    	max-width: calc(100% - 400px);
	    	padding: 3% 3% 3% 0;
			}
	    .swiper-container {
	    	height: 100% !important;
	    }
		}

	}

}

// Navigation buttons

.draw-buttons {

	position: fixed;
	right: 0;
	width: 60px;
	height: 60px;
	overflow: hidden;
	text-align: center;
	margin: 0;
	z-index: 9999;
	@include transition(width, 250ms, linear);

	.mount-product & {
    position: relative;
    margin-top: calc(-6% - 30px);
    float: right;
	}
	.template-index & {
		position: absolute;
	}

	.no-touch &:hover {
		width: 120px;
	}
	.no-touch &.three:hover {
		width: 180px;
	}

	.holder {

		position: absolute;
		width: 180px;

		> span {

			width: 60px;
			height: 60px;
			display: block;
			float: left;
			position: relative;
			@include alphaback(#000, .2);
			@extend %coloranim;

			.no-touch &:hover {
				background: #1d1d1d;
			}

			&.swiper-no:hover {
				@include alphaback(#000, .2);
			}

		}

	}

	.back {
		display: none;
	}
	&:hover .back {
		display: block;
	}
	&:hover .front {
		display: none;
	}

	a {
		position: absolute;
		top: 0;
		left: 0;
		width: 60px;
		height: 60px;
		text-indent: -9999px;
	}

	.svg {

		@extend a;
		fill: #fff;

		.ie8 & {
			color: #fff;
		}

	}
	.svg.grid {
		shape-rendering: crispedges;
	}

	&.nav {
		top: 0;
	}
	&.gal {
		bottom: 0;
	}

	&.nav .holder > span {
		position: absolute;
		left: 60px;
		&.btn-close {
			left: 0;
		}
	}
	&.nav.three {
		span.btn-next {
			left: 120px;
		}
	}

	&.zoom {

		display: none;
		bottom: 61px;
		width: 60px !important;
		cursor: pointer;

		.svg.zoom-out {
			display: none;
		}

		&.zoomed {
			.svg.zoom {
				display: none;
			}
			.svg.zoom-out {
				display: block;
			}
		}

	}

}

// Swiper Slider

.swiper-container {

	height: 100%;

	.swiper-slide {
	  position: relative;
	  overflow: hidden !important;
	  height: 100% !important;
	  > div {
	  	height: 100% !important;
	  }
	}

	img {

		@include opacity(0);
		position: relative;
		width: 100% !important;
		height: 100% !important;
		top: 0 !important;
		left: 0 !important;

		.scale-up-fill & {
	    object-fit: cover;
	    font-family: "object-fit: cover";
		}
		.scale-up-fit-both & {
	    object-fit: contain;
	    font-family: "object-fit: covcontainer";
		}

	}

	.ie8 & {
		cursor: move !important;
	}

	&.allow-mouse {
		pointer-events: all !important;
	}

}

.swiper-no {

	padding-top: 17px;
	cursor: default;

	span {
		color: #fff;
		font: normal 12px/12px $headingsFont;
	}
	.cur:after {
		content: "/ ";
		letter-spacing: -2px;
	}

}

// - fading transition addon

.swiper-container.force-fade {

	cursor: default !important;

	.swiper-wrapper {
		-webkit-transition: none !important;
		transition: none !important;
	}

	.swiper-slide {

		position: absolute !important;
		top: 0;
		left: 0;
		@include opacity(0);

	}

}

.slide.swiper-slide.swiper-slide-active { z-index: 999; }

